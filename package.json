{
  "name": "ui-xxx",
  "version": "0.0.1",
  "description": "Micro Service Name",
  "author": "SSENSE",
  "scripts": {
    "start": "NODE_ENV=production npm run build && node server.js",
    "dev": "NODE_ENV=development webpack-dev-server --color --hot --inline --config ./webpack.dev.config.js --content-base ./public",
    "build": "NODE_ENV=production UV_THREADPOOL_SIZE=100 webpack --config ./webpack.prod.config.js",
    "lint": "./node_modules/.bin/eslint 'app/**'",
    "lint:fix": "./node_modules/.bin/eslint 'app/**' --fix",
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test:unit ./node_modules/.bin/mocha tests/unit --recursive --compilers jsx:babel-core/register,jsx:./tests/compilers/less-compiler.jsx,less:./tests/compilers/less-compiler.jsx --require ./server.js --require ./tests/helpers/setup.jsx --bail",
    "cover": "npm run cover:unit",
    "cover:unit": "NODE_ENV=test:unit ./node_modules/.bin/nyc --report-dir tests/coverage/unit npm run test:unit"
  },
  "dependencies": {
    "antd": "^3.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "body-parser": "^1.18.2",
    "chalk": "^1.1.3",
    "config": "^1.29.0",
    "cors": "^2.8.1",
    "express": "^4.16.2",
    "history": "^4.3.0",
    "lodash": "^4.16.4",
    "node-basicauth": "0.0.9",
    "offline-plugin": "^4.9.0",
    "prop-types": "^15.6.0",
    "randomstring": "^1.1.5",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.2.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "remote-redux-devtools": "^0.5.7",
    "request": "^2.83.0",
    "superagent": "^3.8.2",
    "superagent-promise": "^1.1.0",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.4",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-ui-core": "^0.2.2",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^10.1.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.2",
    "mocha": "^3.5.3",
    "nock": "^9.1.5",
    "nyc": "^10.2.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.10",
    "progress-bar-webpack-plugin": "^1.9.0",
    "react-addons-create-fragment": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^3.1.3",
    "redux-mock-store": "^1.4.0",
    "sinon": "^2.1.0",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.7",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-dev-server": "^2.9.7",
    "webpack-hot-middleware": "^2.21.0"
  },
  "nyc": {
    "exclude": [
      "app/index.jsx"
    ],
    "include": [
      "app/*.js",
      "app/*.jsx",
      "app/**/*.js",
      "app/**/*.jsx"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "require": [
      "babel-core/register"
    ],
    "reporter": [
      "json",
      "html",
      "text-summary"
    ],
    "all": true
  }
}
