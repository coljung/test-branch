{
  "name": "ui-store",
  "version": "1.0.14",
  "description": "Store Dashboard",
  "author": "SSENSE",
  "scripts": {
    "start": "NODE_ENV=production npm run build && node server.js",
    "dev": "NODE_ENV=development webpack-dev-server --config --hot --inline ./webpack.dev.config.js --content-base ./public",
    "build": "NODE_ENV=production UV_THREADPOOL_SIZE=100 webpack --config ./webpack.prod.config.js",
    "lint": "./node_modules/.bin/eslint 'app/**'",
    "lint:fix": "./node_modules/.bin/eslint 'app/**' --fix",
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test:unit ./node_modules/.bin/mocha tests/unit --recursive --compilers jsx:babel-core/register,jsx:./tests/compilers/less-compiler.jsx,less:./tests/compilers/less-compiler.jsx --require ./server.js --require ./tests/helpers/setup.jsx --bail",
    "cover": "npm run cover:unit",
    "cover:unit": "NODE_ENV=test:unit ./node_modules/.bin/nyc --report-dir tests/coverage/unit npm run test:unit"
  },
  "dependencies": {
    "antd": "^2.8.3",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "chart.js": "^2.5.0",
    "config": "^1.24.0",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "history": "^4.3.0",
    "lodash": "^4.16.4",
    "node-basicauth": "0.0.9",
    "offline-plugin": "^4.6.1",
    "randomstring": "^1.1.5",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.4.2",
    "react-chartjs": "^0.8.0",
    "react-dom": "^15.3.0",
    "react-redux": "^5.0.3",
    "react-router": "^2.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "remote-redux-devtools": "^0.5.7",
    "request": "^2.78.0",
    "superagent": "^3.4.1",
    "superagent-promise": "^1.1.0",
    "webpack": "^2.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^9.8.0",
    "less": "^2.7.1",
    "less-loader": "^4.0.2",
    "mocha": "^3.2.0",
    "nock": "^9.0.10",
    "nyc": "^10.2.0",
    "postcss-loader": "^1.2.2",
    "progress-bar-webpack-plugin": "^1.9.0",
    "react-addons-create-fragment": "^15.4.2",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "1.3.1",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-mock-store": "^1.2.1",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.7",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.13.0"
  },
  "nyc": {
    "exclude": [
      "app/index.jsx"
    ],
    "include": [
      "app/*.js",
      "app/*.jsx",
      "app/**/*.js",
      "app/**/*.jsx"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "require": [
      "babel-core/register"
    ],
    "reporter": [
      "json",
      "html",
      "text-summary"
    ],
    "all": true
  }
}
