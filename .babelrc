{
  "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
  ],
  "plugins": [
      ["@babel/plugin-transform-runtime", { "sourceType": "unambiguous" }],
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-modules-commonjs",
      "react-hot-loader/babel",
      ["import", { "libraryName": "antd", "style": true }],
  ]
}
