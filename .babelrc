{
  "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
  ],
  "plugins": [
      ["@babel/plugin-transform-runtime", { "sourceType": "unambiguous" }],
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-modules-commonjs",
      ["@babel/plugin-proposal-decorators", { "legacy": true }],
      "react-hot-loader/babel"
  ]
}
